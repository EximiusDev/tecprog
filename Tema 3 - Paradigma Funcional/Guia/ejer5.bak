; (+(/(*7a)b)(/(*3a)b)(/(*7a)b))      ; 7a/b + 3a/b + 7a/b

(define a 5)   ; Por ejemplo, a = 5
(define b 2)   ; Por ejemplo, b = 2

(+ (/ (* 7 a) b) 
   (/ (* 3 a) b) 
   (/ (* 7 a) b))

(let ((sub_expr1 (/ (* 7 a) b))  ; sub_expr1 = (/ (* 7 5) 2) = 17.5
      (sub_expr2 (/ (* 3 a) b))) ; sub_expr2 = (/ (* 3 5) 2) = 7.5
  (+ sub_expr1 sub_expr2 sub_expr1)) ; (+ 17.5 7.5 17.5) → 42.5

